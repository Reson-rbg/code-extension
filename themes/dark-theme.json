{
	"name": "Themesite Dark Redesign",
	"type": "dark",
	"colors": {
		"editor.background": "#282A36",
		"activityBar.background": "#1f1f1f",
		"activityBar.foreground": "#ff9800",
		"sideBar.background": "#1a1a1a",
		"sideBar.foreground": "#ff9800",
		"statusBar.background": "#4b0082",
		"statusBar.foreground": "#ffffff",
		"editorGroupHeader.tabsBackground": "#1a1a1a",
		"tab.activeBackground": "#121212",
		"tab.inactiveBackground": "#292929",
		"tab.border": "#1a1a1a",
		"button.background": "#4b0082",
		"button.foreground": "#ffffff",
		"list.activeSelectionBackground": "#37474f",
		"list.hoverBackground": "#263238"
	},
	"tokenColors": [
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#9e9e9e",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#ff9800",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#ffcc80"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#c5e1a5"
			}
		},
		{
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#80deea"
			}
		},
		{
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#ffd54f"
			}
		}
	]
}